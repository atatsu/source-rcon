>>> import socket
>>> from srcrcon  import packet
>>> p = packet.ServerDataAuth('cadderly')
>>> p.id
995
>>> s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
>>> s.connect(('209.170.149.170', 24552))
>>> s.send(bytes(p))
22
>>> s.recv(1024)
b'\n\x00\x00\x00\xff\xff\xff\xff\x02\x00\x00\x00\x00\x00'
>>> s.recv(1024)
b''
>>> s.recv(1024)
b''
>>> s.close()
